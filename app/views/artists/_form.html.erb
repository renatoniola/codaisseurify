<%= form_for @artist_item, html: { multipart: true } do |f| %>

<label>Name :</label>
<%= f.text_field :name, placeholder: "insert name" %>
<%= f.text_area :bio, placeholder: "insert bio" %>

Upload Photo
<%= file_field_tag "images[]", type: :file, multiple: true %>

<%= f.submit "create new artist" %>

<% end %>
<hr>

<% if @photos %>
<% @photos.each do |photo| %>

  <%= image_tag photo.image %>

<% end %>
<% end %>
